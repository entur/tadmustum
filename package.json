{
  "name": "tadmustum",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "run-p --race copy:theme copy:config",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "copy:theme": "ncp .github/environments/theme-dev.json public/theme-dev.json",
    "copy:config": "ncp .github/environments/config-local.json public/config.json",
    "lint": "eslint .",
    "check": "prettier . --check",
    "format": "prettier --write .",
    "prepare": "husky",
    "preview": "vite preview"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^5.0.1",
    "@mapbox/mapbox-gl-draw": "^1.4.3",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@mui/x-data-grid": "^8.2.0",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "@mui/x-date-pickers": "^8.3.0",
    "dayjs": "^1.11.13",
    "graphql": "^16.11.0",
    "maplibre-gl": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "oidc-client-ts": "^3.2.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "react-i18next": "^15.5.2",
    "react-map-gl": "^8.0.4",
    "react-oidc-context": "^3.3.0",
    "react-router-dom": "^6.30.0",
    "uuid": "^11.1.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/mapbox__mapbox-gl-draw": "^1.4.3",
    "@types/node": "^22.15.12",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "ncp": "^2.0.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
